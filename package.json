{
    "name": "mharj-filter-schema-util",
    "version": "0.1.1",
    "description": "Filter utility with schemas",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "test": "nyc mocha"
    },
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@types/chai": "^4.2.14",
        "@types/mocha": "^8.2.0",
        "@types/node": "^14.14.21",
        "chai": "^4.2.0",
        "mocha": "^8.2.1",
        "nyc": "^15.1.0",
        "source-map-support": "^0.5.19",
        "ts-node": "^9.1.1",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.1.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mharj/filter-schema-util.git"
    },
    "keywords": [
        "filter",
        "utility",
        "typescript",
        "javascript",
        "schema"
    ],
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ]
    },
    "nyc": {
        "extension": [
            ".ts"
        ],
        "include": [
            "src"
        ],
        "reporter": [
            "text",
            "lcovonly"
        ],
        "all": true
    },
    "author": "mharj",
    "license": "LGPL-3.0-or-later",
    "bugs": {
        "url": "https://github.com/mharj/filter-schema-util/issues"
    },
    "homepage": "https://github.com/mharj/filter-schema-util#readme"
}
