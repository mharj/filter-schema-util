{
  "name": "mharj-filter-schema-util",
  "version": "0.1.1",
  "description": "Filter utility with schemas",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "test": "nyc mocha test/**/*.ts"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.12.44",
    "chai": "^4.2.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mharj/filter-schema-util.git"
  },
  "keywords": [
    "filter",
    "utility",
    "typescript",
    "javascript",
    "schema"
  ],
  "mocha": {
    "exit": true,
    "recursive": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src"
    ],
    "reporter": [
      "text",
      "lcovonly"
    ],
    "all": true
  },
  "author": "mharj",
  "license": "LGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/mharj/filter-schema-util/issues"
  },
  "homepage": "https://github.com/mharj/filter-schema-util#readme"
}
